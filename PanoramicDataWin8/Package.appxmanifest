    <DisplayName>PanoramicDataWin8</DisplayName>
    <PublisherDisplayName>Emanuel</PublisherDisplayName>
    <Logo>Assets/img/logo1000x100.png</Logo>
  </Properties>
  <Prerequisites>
    <OSMinVersion>6.3.0</OSMinVersion>
    <OSMaxVersionTested>6.3.0</OSMaxVersionTested>
